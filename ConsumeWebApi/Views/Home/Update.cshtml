@model ConsumeWebApi.Models.PersonEntity

@{
    Layout = null;
   // ViewData["Title"] = "Update";
}
<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>

    <div class="row ">
        <div class="col-md-2">
        </div>
        <div class="col-md-8 ">
            <form asp-action="UpdatePerson">
                <h1 class="text-center">Update Person</h1>
                <div class="mb-3">
                    <label class="form-label">Id</label>
                    <input asp-for="person_Id" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" readonly>

                </div>
                <div class="mb-3">
                    <label class="form-label">Name</label>
                    <input asp-for="person_Name" class="form-control" >
                </div>
                <div class="mb-3">
                    <label class="form-label">Email</label>
                    <input asp-for="person_Email" class="form-control">
                </div>
                <div class="mb-3">
                    <label class="form-label">Address</label>
                    <input asp-for="person_Address" class="form-control" >
                </div>
                <div class="mb-3">
                    <label class="form-label">Phone</label>
                    <input asp-for="person_Phone" class="form-control" >
                </div>

@*                <button type="submit" class="btn btn-primary">Update</button>*@
                    <input type="submit" value="Update" class="btn btn-primary" />
            </form>



        </div>
        <div class="col-md-2"></div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
</body>
</html>